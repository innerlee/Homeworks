// 将设置放入此文件中以覆盖默认值和用户设置。
{
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "*.sty": true,
    "*.out": true,
    "*.js": true,
    "*.aux": true,
    "*.dvi": true,
    "*.fls": true,
    "*.bbl": true,
    "*.bst": true,
    "*.log": true,
    "*.blg": true,
    "main.pdf": true,
    "bf.pdf": true,
    "*.vscodeLog": true,
    ".gitignore": true,
    ".vscode": true,
    "*.fdb_latexmk": true,
    "*.synctex": true,
    "*.synctex.gz": true,
    "*.synctex.gz(busy)": true,
    "*.pdfsync": true,
    "node_modules": true
  },
  "spellchecker.language": "en_US",
  "spellchecker.ignoreWordsList": [
    "Schlumberger",
    "Kaufmann",
    "Jowell",
    "ctmc",
    "wcomb",
    "CIFAR",
    "ccccc",
    "ijcai17",
    "ijcai07",
    "n01317541",
    "n03575240",
    "n00015388",
    "n00020090",
    "n03297735",
    "cccccc",
    "G123",
    "prob1",
    "prob2",
    "prob3",
    "prob4",
    "medskip",
    "natbib",
    "nonatbib",
    "hyperlinks",
    "microtypography",
    "CrossEntropy",
    "Zhizhong",
    "CUHK",
    "ELEG",
    "subgradient",
    "indices",
    "Gaussians",
    "regularizers",
    "subdifferentiable",
    "nullspace"
  ],
  "spellchecker.documentTypes": [
    "markdown",
    "latex",
    "plaintext"
  ],
  "spellchecker.ignoreRegExp": [
    "/\\\\\\\\\\\\w+/g",
    "/\\\\w+\\\\{/g",
    "/\\\\w+'/g",
    "/\\\\w+''/g",
    "/fig:\\\\w+/g",
    "/eq:\\\\w+/g",
    "/tab:\\\\w+/g",
    "/\\\\w+.png/g",
    "/\\\\w+.jpg/g",
    "/\\\\{\\\\w+\\\\}/g",
    "/\\\\{\\\\w+:\\\\w+\\\\}/g",
    "/\\\\[\\\\w+\\\\]/g",
    "/fig\\/\\\\w+/g",
    "/\\\\w+.pdf/g"
  ],
  "spellchecker.ignoreFileExtensions": [
    ".bib",
    ".pdf"
  ],
  "spellchecker.checkInterval": 5000
}
