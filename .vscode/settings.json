// 将设置放入此文件中以覆盖默认值和用户设置。
{
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "*.sty": true,
    "*.out": true,
    "*.js": true,
    "*.aux": true,
    "*.dvi": true,
    "*.fls": true,
    "*.bbl": true,
    "*.bst": true,
    "*.log": true,
    "*.blg": true,
    "main.pdf": true,
    "bf.pdf": true,
    "*.vscodeLog": true,
    ".gitignore": true,
    ".vscode": true,
    "*.fdb_latexmk": true,
    "*.synctex": true,
    "*.synctex.gz": true,
    "*.synctex.gz(busy)": true,
    "*.pdfsync": true,
    "node_modules": true
  }
}